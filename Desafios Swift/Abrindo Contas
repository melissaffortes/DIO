   // var saldoInicial = Double(readLine() ?? "")
}*/

    print("Informacoes:")
    print("Conta: \(numeroConta)")
    print("Titular: \(nomeTitular)")
    print("Saldo: R$ \(saldo)")
    //TODO: Imprimir as informações da conta usando o objeto criado no TODO acima.


